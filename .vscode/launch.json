    "version": "0.2.0",
    "configurations": [
      // Example launch of a python file
      {
        "name": "Python: Current File",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "justMyCode": false,
        "purpose": ["debug-in-terminal"]
  
      },
